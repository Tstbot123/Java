<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" pickOnBounds="false" prefHeight="625.0" prefWidth="764.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="org.apps.advancewars.controllers.MapSelectionController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="MAP SELECTION">
            <font>
                <Font size="36.0" />
            </font>
        </Text>
        <ImageView disable="true" fitHeight="477.0" fitWidth="764.0" focusTraversable="true" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/map/background.png" />
            </image>
        </ImageView>
        <HBox alignment="CENTER" spacing="10">
            <children>
                <Button fx:id="eonSpringsButton" onAction="#handleEonSprings" text="EonSprings" />
                <Button fx:id="littleIslandButton" onAction="#handleLittleIsland" text="LittleIsland" />
                <Button fx:id="pistonDamButton" onAction="#handlePistonDam" text="PistonDam" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
